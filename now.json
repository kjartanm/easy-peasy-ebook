{
    "version": 2,
    "builds": [
        {
            "src": "./docs/dist/*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "./ebook.epub"
        },
        {
            "src": "/pdf",
            "dest": "./*.pdf"
        },
        {
            "src": "/report",
            "dest": "./epubcheck.json"
        }
    ]
}